<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.mso.mapper.Fbf_demandMapper">
	
	<select id="Fbf_demandList" resultType="map" parameterType="com.stylefeng.guns.modular.mso.model.Fbf_demand">
		select * from fbf_demand t
		<where>
			<if test="id != null and id != ''">
			and	t.id LIKE CONCAT(CONCAT('%', #{id}),'%')
		</if>
		<if test="demand_name != null and demand_name != ''">
			and	t.demand_name LIKE CONCAT(CONCAT('%', #{demand_name}),'%')
		</if>
		</where>
	</select>

</mapper>