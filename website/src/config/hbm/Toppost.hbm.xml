<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mso.model.Toppost" table="mk_toppost">
		<id name="topid" type="java.lang.String">
			<column name="topid"/>
			<generator class="assigned"/>    
		</id>
		<property name="type" type="java.lang.String">
			<column name="type" length="100" not-null="true">
			</column>
		</property>
		<property name="topdirection" type="java.lang.String">
			<column name="topdirection" length="50">
			</column>
		</property>
		<property name="title" type="java.lang.String">
			<column name="title" not-null="true">
			</column>
		</property>
		<property name="topphotourl" type="java.lang.String">
			<column name="topphotourl" not-null="true">
			</column>
		</property>
		<property name="photodescrip" type="java.lang.String">
			<column name="photodescrip" not-null="true">
			</column>
		</property>
		<property name="createtime" type="java.lang.String">
			<column name="createtime" length="20">
			</column>
		</property>
		<property name="updatetime" type="java.lang.String">
			<column name="updatetime" length="20" >
			</column>
		</property>
		<property name="url" type="java.lang.String">
			<column name="url">
			</column>
		</property>
	
	</class>
</hibernate-mapping>