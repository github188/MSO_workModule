<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mso.model.BConverRecord" table="b_conver_record" >
         <id name="crid" type="java.lang.String"> 
             <column name="crid" length="100" />
              <generator class="assigned" />
         </id> 
        <property name="jfuid" type="java.lang.String">
            <column name="jfuid" />
        </property>
        <property name="visitid" type="java.lang.String">
            <column name="visitid" />
        </property>
        <property name="crtype" type="java.lang.String">
            <column name="crtype" />
        </property>
        <property name="createtime" type="java.lang.String">
            <column name="createtime" />
        </property>
        <property name="updatetime" type="java.lang.String">
            <column name="updatetime" />
        </property>
        <property name="cookie" type="java.lang.String">
            <column name="cookie" />
        </property>
        
    </class>
    
</hibernate-mapping>
